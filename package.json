{
  "name": "dzelda-backend",
  "version": "1.0.0",
  "description": "Backend of Dzelda: A free and open source language learning site.",
  "main": "build/src/main.js",
  "type": "module",
  "scripts": {
    "pretest": "npm run migrate",
    "test": "npx vitest --no-watch",
    "compile": "npx swc src -d ./build/src &&  npx swc tests -d ./build/tests",
    "clean": "rm -rf ./build/* && npm run compile",
    "premake-migrations": "npm run clean",
    "make-migrations": "mikro-orm migration:create",
    "premigrate": "npm run clean",
    "migrate": "mikro-orm migration:up",
    "prefresh": "npm run clean",
    "fresh": "mikro-orm migration:fresh --seed",
    "drop": "mikro-orm schema:drop --run",
    "dev": "env-cmd -f ./env/dev.env tsx --watch ./src/main.ts",
    "temp": "tsx test.ts"
  },
  "author": "squivix <devintheamateurdevin@gmail.com> (https://github.com/squivix)",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.489.0",
    "@aws-sdk/s3-presigned-post": "^3.489.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/one-line-logger": "^1.0.0",
    "@fastify/static": "^6.6.1",
    "@mikro-orm/core": "^5.6.7",
    "@mikro-orm/postgresql": "^5.6.7",
    "aws-sdk": "^2.1533.0",
    "bcrypt": "^5.1.1",
    "dzelda-common": "git+https://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:x-oauth-basic@github.com/squivix/dzelda-common.git#main",
    "fastify": "^4.9.2",
    "fastify-formidable": "^3.0.2",
    "file-type": "^18.2.0",
    "fs-extra": "^11.1.0",
    "http-status-codes": "^2.2.0",
    "image-size": "^1.0.2",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.4",
    "pretty-bytes": "^6.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@mikro-orm/cli": "^5.6.7",
    "@mikro-orm/migrations": "^5.6.7",
    "@mikro-orm/seeder": "^5.6.7",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@types/bcrypt": "^5.0.0",
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^11.0.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.11.2",
    "@types/nodemailer": "^6.4.9",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cli-progress": "^3.12.0",
    "depcheck": "^1.4.3",
    "env-cmd": "^10.1.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "form-data": "^4.0.0",
    "prompts": "^2.4.2",
    "tsx": "^3.12.7",
    "typescript": "^4.8.4",
    "vite": "^3.2.5",
    "vitest": "^0.33.0"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/src/mikro-orm.config.js"
    ]
  }
